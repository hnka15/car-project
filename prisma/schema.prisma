generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Propriety {
  cod_imovel String @id
  mod_fiscal Float
  num_area   Float
  municipio  String
  nome_propr String
  cpf_cnpj   String
  proprietar String
  situacao   String
  shape_leng Float
  shape_area Float

  @@map("propriety")
}
